{
  "name": "rhf-app",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "sources": [
    {
      "dir": "__tests__",
      "type": "dev"
    },
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".mjs",
  "bs-dependencies": [
    "@rescript/react"
  ],
  "bs-dev-dependencies": [],
  "ppx-flags": [
    "../ppx"
  ],
  "warnings": {
    "error": true,
    "number": "-48"
  },
  "generators": [],
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "gentypeconfig": {
    "language": "typescript",
    "shims": {},
    "generatedFileExtension": ".gen.tsx",
    "module": "es6",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
